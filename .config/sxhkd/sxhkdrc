super + Return
  $TERMINAL

super + a
  dmenu_run

super + b
  pkill -USR1 redshift

super + shift + b
  blueberry

super + d
  discord

super + shift + d
  signal-desktop

super + e
  $TERMINAL -e nvim

super + shift + e
  $TERMINAL -e neomutt

super + r
  $TERMINAL -e tuir

super + shift + r
  $TERMINAL -e newsboat

super + s
  dmainu

super + shift + s
  passmenu

super + t
  $TERMINAL -e reflex-curses

super + shift + t
  chatty

super + w
  $BROWSER

super + shift + w
  $TERMINAL -e sudo nmtui

Print
  screenshot_select

XF86MonBrightnessDown
  xbacklight -dec 10

XF86MonBrightnessUp
  xbacklight -inc 10

XF86AudioMute
  amixer sset Master toggle

XF86Audio{Raise,Lower}Volume
  amixer sset Master 5%{+,-}

XF86AudioMicMute
  amixer set Capture toggle

XF86AudioPlay
  mpc play

XF86AudioStop
  mpc pause

XF86AudioNext
  mpc next

XF86AudioPrev
  mpc prev

super + Prior
  mpc volume +5

super + Next
  mpc volume -5
