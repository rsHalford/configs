# <-TGB reserved for apps. YHN & 1-0 -> for dwm.
# Super;              = Win/Mod4Key
      return          = Terminal            (st)
shift return          = Move to Master      (dwm)
      a               = All Applications    (dmenu_run_i)
shift a               = Music Scratchpad    (ncmpcpp)
      b               = Blue Filter         (redshift)
shift b               = GUI Bluetooth       (blueberry)
      c               = Calendar Scratchpad (wyrd)
shift c               = Clipboard Manager   (clipmenu)
      d               = Messenger           (Discord)
shift d               = Private Messenger   (Signal)
      e               = Editor              (neovim)
shift e               = Email               (neomutt)
      f               =
      g               = Game Library        (steam)
shift g               = Terminal Scratchpad (dwm)
      h               = Reduce H Master     (dwm)
shift h               = Rotate Master       (dwm)
      i               = Float Layout        (dwm)
      j               = Increase V Master   (dwm)
shift j               = Focus Previous      (dwm)
      k               = Reduce V Master     (dwm)
shift k               = Focus Next          (dwm)
      l               = Increase H Master   (dwm)
shift l               = Rotate  Master      (dwm)
      m               =
      n               =
      o               = Monocle Layout      (dwm)
      p               = Previous Layout     (dwm)
shift p               = Tag Layout          (dwm)
      q               = Quit Program        (dwm)
shift q               = Logout of X         (dwm)
      r               = Emulators           (RetroArch)
shift r               = RSS Feeds           (Newsboat)
      s               = Dmenu Menu          (dmainu)
shift s               = Passwords           (passmenu)
      t               = Twitch              (reflex-curses)
shift t               = Torrent             (qBittorrent)
      u               = Master Layout       (dwm)
      v               = Audio Scratchpad    (pulsemixer)
shift v               = Virtual Machines    (virt-manager)
      w               = GUI Browser         (qutebrowser)
shift w               = GUI WiFi            (nmtui)
      x               =
shift x               = Crypto Ledger       (Ledger)
      y               =
shift y               = Toggle Taskbar      (dwm)
      z               =
      1-9             = Change Tag          (dwm)
shift 1-9             = Move to Tag         (dwm)
      0               = Show All Tags       (dwm)
shift 0               = Tag to All Tags     (dwm)
      tab             = Previous Tag        (dwm)
      ,               = Focus < Screen      (dwm)
shift ,               = Tag < Screen        (dwm)
      .               = Focus > Screen      (dwm)
shift .               = Tag > Screen        (dwm)
      PageUp          = Music Volume Up     (mpc)
      PageDown        = Music Volume Down   (mpc)

